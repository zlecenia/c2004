{
  "name": "identification-frontend",
  "version": "1.0.0",
  "description": "Identification Service Frontend",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "test:legacy": "node tests/run-tests.js",
    "test:structure": "node tests/test-structure.js",
    "test:modules": "node tests/test-modules.js",
    "test:quality": "node tests/test-code-quality.js",
    "lint": "node tests/test-code-quality.js",
    "lint:fix": "node tests/lint-autofix.js",
    "lint:safe-fix": "node tests/lint-safe-fix.js",
    "lint:dry-run": "node tests/lint-autofix.js --dry-run"
  },
  "dependencies": {
    "lucide": "^0.545.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@types/node": "^20.8.0",
    "@vitest/coverage-v8": "^2.1.0",
    "@vitest/ui": "^2.1.0",
    "happy-dom": "^15.7.4",
    "jsdom": "^25.0.1",
    "typescript": "^5.2.2",
    "vite": "^7.1.9",
    "vitest": "^2.1.0"
  }
}
