# Fleet Management System Environment Configuration

# Database Configuration
POSTGRES_PASSWORD=fleet_password_change_in_production
POSTGRES_USER=fleet_user
POSTGRES_DB=fleet_db

# Backend Services Security
SECRET_KEY_FDM=your-secret-key-fdm-change-in-production-with-32-chars-min
SECRET_KEY_FWM=your-secret-key-fwm-change-in-production-with-32-chars-min
SECRET_KEY_FCM=your-secret-key-fcm-change-in-production-with-32-chars-min

# Service Ports
FRONTEND_PORT=8100
FDM_PORT=8200
FWM_PORT=8201
FCM_PORT=8202
CDN_PORT=9000
POSTGRES_PORT=5432
REDIS_PORT=6379

# CORS Origins (comma separated)
CORS_ORIGINS=http://localhost:8100,http://localhost:8110,http://localhost:8111,http://localhost:8112

# Service URLs (internal Docker network)
FDM_URL=http://fleet-data-manager:8000
FWM_URL=http://fleet-workshop-manager:8000
FCM_URL=http://fleet-config-manager:8000

# External URLs (for frontend)
VITE_FDM_URL=http://localhost:8200
VITE_FWM_URL=http://localhost:8201
VITE_FCM_URL=http://localhost:8202
VITE_CDN_URL=http://localhost:9000

# Redis Configuration
REDIS_URL=redis://redis:6379

# Environment
ENVIRONMENT=development
LOG_LEVEL=info

# Feature Flags
VITE_ENABLE_RFID=true
VITE_ENABLE_QR=true
VITE_ENABLE_BARCODE=true
VITE_ENABLE_MANUAL=true
